#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/g/web/vanila/vite-project/node_modules/.pnpm/esbuild@0.13.15/node_modules/esbuild/bin/node_modules:/mnt/g/web/vanila/vite-project/node_modules/.pnpm/esbuild@0.13.15/node_modules/esbuild/node_modules:/mnt/g/web/vanila/vite-project/node_modules/.pnpm/esbuild@0.13.15/node_modules:/mnt/g/web/vanila/vite-project/node_modules/.pnpm/node_modules:/mnt/g/web/vanila/vite-project/node_modules:/mnt/g/web/vanila/node_modules:/mnt/g/web/node_modules:/mnt/g/node_modules:/mnt/g/web/vanila/vite-project/node_modules/.pnpm/vite@2.7.10/node_modules/vite/node_modules:/mnt/g/web/vanila/vite-project/node_modules/.pnpm/vite@2.7.10/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/g/web/vanila/vite-project/node_modules/.pnpm/esbuild@0.13.15/node_modules/esbuild/bin/node_modules:/mnt/g/web/vanila/vite-project/node_modules/.pnpm/esbuild@0.13.15/node_modules/esbuild/node_modules:/mnt/g/web/vanila/vite-project/node_modules/.pnpm/esbuild@0.13.15/node_modules:/mnt/g/web/vanila/vite-project/node_modules/.pnpm/node_modules:/mnt/g/web/vanila/vite-project/node_modules:/mnt/g/web/vanila/node_modules:/mnt/g/web/node_modules:/mnt/g/node_modules:/mnt/g/web/vanila/vite-project/node_modules/.pnpm/vite@2.7.10/node_modules/vite/node_modules:/mnt/g/web/vanila/vite-project/node_modules/.pnpm/vite@2.7.10/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../esbuild@0.13.15/node_modules/esbuild/bin/esbuild" "$@"
else
  exec node  "$basedir/../../../../../esbuild@0.13.15/node_modules/esbuild/bin/esbuild" "$@"
fi
